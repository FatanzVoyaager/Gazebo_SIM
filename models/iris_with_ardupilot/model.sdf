<?xml version='1.0'?>
<sdf version="1.6">
  <model name="iris_demo">
    <include>
      <uri>model://iris_with_standoffs</uri>
    </include>

    <include>
      <uri>model://gimbal_small_2d</uri>
      <pose>0 -0.01 0.070 1.57 0 1.57</pose>
    </include>

    <joint name="iris_gimbal_mount" type="revolute">
      <parent>iris::base_link</parent>
      <child>gimbal_small_2d::base_link</child>
      <axis>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
        <xyz>0 0 1</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>

    <link name="rplidar">
      <pose>0 0 0.25 0 0 0</pose>
      <inertial>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.0001</iyy>
          <iyz>0.000</iyz>
          <izz>0.0001</izz>
        </inertia>
        <mass>0.114</mass>
      </inertial>

      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.03</radius>
            <length>0.03</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.03</radius>
            <length>0.03</length>
          </cylinder>
          <!-- <include>
            <mesh>
              <uri>model://turtlebot3_common/meshes/sensors/lds.stl</uri>
              <scale>0.01 0.01 0.01</scale>
            </mesh>
          </include> -->
        </geometry>
        <material>
          <!-- <ambient>1 1 1 1</ambient> -->
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.5 0.5 0.5 1</diffuse>
        </material>
      </visual>

      <sensor name="rplidar_sensor" type="ray">
        <visualize>true</visualize>
        <pose>0 0 0 0 0 0</pose>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>720</samples>
              <resolution>1.000000</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>6.0</max>
            <resolution>0.1</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <alwaysOn>1</alwaysOn>
        <plugin name="gazebo_ros_laser" filename="libgazebo_ros_laser.so">
          <robotNamespace>/rplidar</robotNamespace>
          <topicName>scan</topicName>
          <frameName>rplidar</frameName>
        </plugin>
      </sensor>
      <!-- <static>true</static> -->
    </link>

    <joint name="rplidar_joint" type="fixed">
      <parent>iris::base_link</parent>
      <child>rplidar</child>
    </joint>
      
    <link name="rangefinder">
      <pose>0.1 -0.15 0.18 0 1.5708 0</pose>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.04 0.04 0.04</size>
          </box>
        </geometry>
      </collision>
      
      <visual name="visual">
        <geometry>
          <box>
            <size>0.02 0.02 0.02</size>
          </box>
        </geometry>
        <material>
          <!-- <ambient>0.8 0.2 0.2 1</ambient> -->
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.8 0.2 0.2 1</diffuse>
        </material>
        </visual>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.001</ixx>
              <ixy>0</ixy>
              <ixz>0</ixz>
              <iyy>0.001</iyy>
              <iyz>0</iyz>
              <izz>0.001</izz>
            </inertia>
          </inertial>
          
      <sensor name="rangefinder_sensor" type="range">
        <visualize>true</visualize>  <!-- ROTASI sensor ke bawah -->
          <update_rate>20</update_rate>
          <ray>
            <scan>
                <horizontal>
                  <samples>10</samples>
                  <resolution>1</resolution> 
                  <min_angle>-0.14835</min_angle>
                  <max_angle>0.14835</max_angle>
                </horizontal>
                <vertical>
                  <samples>10</samples>
                  <resolution>1</resolution> 
                  <min_angle>-0.12835</min_angle>
                  <max_angle>0.12835</max_angle> 
                </vertical>
            </scan>
            <range>
                <min>0.05</min>
                <max>7</max>
                <resolution>0.02</resolution>
            </range>
            <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.01</stddev>
            </noise>
          </ray>
          <!-- <plugin name="gazebo_ros_api_plugin" filename="libgazebo_ros_api_plugin.so"> -->
          <plugin name="gazebo_ros_range" filename="libgazebo_ros_range.so">
            <gaussianNoise>0.005</gaussianNoise>
            <alwaysOn>true</alwaysOn>
            <updateRate>50</updateRate>
            <robotNamespace>/rangefinder</robotNamespace>
            <!-- <topicName>/mavros/rangefinder/rangefinder</topicName> -->
            <topicName>/mavros/distance_sensor/rangefinder</topicName>
            <rangefinderName>rangefinder</rangefinderName>
            <frameName>rangefinder</frameName>
            <radiation>INFRARED</radiation>
            <fov>0.1745</fov>
          </plugin>
      </sensor>
    </link>

    <joint name="rangefinder_joint" type="fixed">
      <parent>iris::base_link</parent>
      <child>rangefinder</child>
      <axis>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
        <xyz>0 0 1</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    
